   "source": [
    "#Check for prime or composite\n",
    "n=int(input(\"Enter a number:\"))\n",
    "def prime(n):\n",
    "    for x in range(2,n/2+1):\n",
    "        if n % x == 0:\n",
    "            return \"Composite\"\n",
    "    \n",
    "    return  \"Prime\"\n",
    "\n",
    "print (prime(n))"
   ]
